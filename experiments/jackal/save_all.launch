<launch>

  <arg name="ycb_item" />
  <arg name="number" />

<include file="$(dirname)/save_images.launch">
  <arg name="ycb_item" value="$(arg ycb_item)" />
  <arg name="number" value="$(arg number)" />
</include>

<node pkg="rosbag" type="play" name="rosbag" required="true" args="--delay=1 --clock $(dirname)/bags/ziqi/$(arg ycb_item)_$(arg number).bag"/>

</launch>